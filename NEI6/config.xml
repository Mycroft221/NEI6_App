<?xml version='1.0' encoding='utf-8'?>
<widget id="org.nei6.ios" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="apple-touch-icon.png" />
    <name>Trauma Intervention Prediction</name>
    <content src="https://nei6.ddns.net/" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="https://nei6.ddns.net/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-targetSdkVersion" value="29" />
    </platform>
    <platform name="ios">
	<allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="cordova-plugin-splashscreen" spec="~6.0.0" />
</widget>
